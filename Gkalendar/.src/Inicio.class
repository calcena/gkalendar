        For i = 1 To DiasDelMes
          conn.Exec("update actualkal set JULIO =" & i & " where id=" & NumDiaSemana) 
          NumDiaSemana += 1   
        Next
        'AGOSTO 
      Case 8
        DiasDelMes = 31
        'Número de dia de la semana del día 1 de AGOSTO
        NumDiaSemana = WeekDay(CDate("08/01/" & Str(anyo))) + 1 
        ' Iteramos los días de cada mes desde el 1 hasta Dias del Mes
        For i = 1 To DiasDelMes
          conn.Exec("update actualkal set AGOSTO =" & i & " where id=" & NumDiaSemana) 
          NumDiaSemana += 1   
        Next
        'SEPTIEMBRE 
      Case 9
        DiasDelMes = 30
        'Número de dia de la semana del día 1 de SEPTIEMBRE
        NumDiaSemana = WeekDay(CDate("09/01/" & Str(anyo))) + 1
        ' Iteramos los días de cada mes desde el 1 hasta Dias del Mes
        For i = 1 To DiasDelMes
          conn.Exec("update actualkal set SEPTIEMBRE =" & i & " where id=" & NumDiaSemana) 
          NumDiaSemana += 1   
        Next
        'OCTUBRE 
      Case 10
        DiasDelMes = 31
        'Número de dia de la semana del día 1 de OCTUBRE
        NumDiaSemana = WeekDay(CDate("10/01/" & Str(anyo))) + 1
        ' Iteramos los días de cada mes desde el 1 hasta Dias del Mes
        For i = 1 To DiasDelMes
          conn.Exec("update actualkal set OCTUBRE =" & i & " where id=" & NumDiaSemana) 
          NumDiaSemana += 1   
        Next
        'NOVIEMBRE 
      Case 11
        DiasDelMes = 30
        'Número de dia de la semana del día 1 de NOVIEMBRE
        NumDiaSemana = WeekDay(CDate("11/01/" & Str(anyo))) + 1 
        ' Iteramos los días de cada mes desde el 1 hasta Dias del Mes
        For i = 1 To DiasDelMes
          conn.Exec("update actualkal set NOVIEMBRE =" & i & " where id=" & NumDiaSemana) 
          NumDiaSemana += 1   
        Next
        'DICIEMBRE 
      Case 12
        DiasDelMes = 31
        'Número de dia de la semana del día 1 de DICIEMBRE
        NumDiaSemana = WeekDay(CDate("12/01/" & Str(anyo))) + 1
        ' Iteramos los días de cada mes desde el 1 hasta Dias del Mes
        For i = 1 To DiasDelMes
          conn.Exec("update actualkal set DICIEMBRE =" & i & " where id=" & NumDiaSemana) 
          NumDiaSemana += 1   
        Next
      Case Else
        Message.Error("Error inesperado en generación del código")
    End Select    
  Next
  
  conn.Commit
  ' Mostramos los Botones para generar los informes
  btnPrimero.Visible = True
  btnSegundo.Visible = True
  btnTercero.Visible = True 
  
  'Se oculta mensaje de Generando calendario...
  txtTrabajo.visible = False
  
  btnAceptar.Enabled = True
  
  ' Cerramos la conexion
  conn.Close
  
End

