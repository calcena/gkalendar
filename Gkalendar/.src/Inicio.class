Public Sub Form_Open()
  ' Comprobamos que exista la carpeta con la DB SQlite  
  
  If Not Exist(User.Home &/ ".gKalendar/jKalendar")
    Try Mkdir User.Home &/ ".gKalendar"
    Copy "db/jKalendar" To User.Home &/ ".gKalendar/jKalendar"
  Endif
  
  ' Inicializamos la DB
  Init()
  
  txtAnyo.text = Year(Now) + 1
  btnAceptar.SetFocus
  
End

Public Sub Init()
  
  ' Abrimos la conexión a DB
  conn = gConnection.ConectarDb()
  
  conn.Exec("update actualkal set ENERO =0  where id >0") 
  conn.Exec("update actualkal set FEBRERO =0  where id >0") 
  conn.Exec("update actualkal set MARZO =0  where id >0") 
  conn.Exec("update actualkal set ABRIL =0  where id >0") 
  conn.Exec("update actualkal set MAYO =0  where id >0") 
  conn.Exec("update actualkal set JUNIO =0  where id >0") 
  conn.Exec("update actualkal set JULIO =0  where id >0") 
  conn.Exec("update actualkal set AGOSTO =0  where id >0") 
  conn.Exec("update actualkal set SEPTIEMBRE =0  where id >0") 
  conn.Exec("update actualkal set OCTUBRE =0  where id >0") 
  conn.Exec("update actualkal set NOVIEMBRE =0  where id >0") 
  conn.Exec("update actualkal set DICIEMBRE =0  where id >0") 
  ' Festivos
  conn.Exec("update actualkal set FENERO =0  where id >0") 
  conn.Exec("update actualkal set FFEBRERO =0  where id >0") 
  conn.Exec("update actualkal set FMARZO =0  where id >0") 
  conn.Exec("update actualkal set FABRIL =0  where id >0") 
  conn.Exec("update actualkal set FMAYO =0  where id >0") 
  conn.Exec("update actualkal set FJUNIO =0  where id >0") 
  conn.Exec("update actualkal set FJULIO =0  where id >0") 
  conn.Exec("update actualkal set FAGOSTO =0  where id >0") 
  conn.Exec("update actualkal set FSEPTIEMBRE =0  where id >0") 
  conn.Exec("update actualkal set FOCTUBRE =0  where id >0") 
  conn.Exec("update actualkal set FNOVIEMBRE =0  where id >0") 
  conn.Exec("update actualkal set FDICIEMBRE =0  where id >0") 
  
  conn.Commit
  
  rIdioma = conn.Exec("select numero from config where seleccion = 1 ")
  
  rIdioma.MoveFirst
  Do While rIdioma.Available
    Select Case rIdioma!numero
      Case 1
        rdbSp.value = 1
        btnPrimero.Text = gDatosEstaticos.informe_1_sp
        btnSegundo.text = gDatosEstaticos.informe_2_sp
        btnTercero.Text = gDatosEstaticos.informe_3_sp
      Case 2
        rdbCat.value = 1
        btnPrimero.Text = gDatosEstaticos.informe_1_cat
        btnSegundo.text = gDatosEstaticos.informe_2_cat
        btnTercero.Text = gDatosEstaticos.informe_3_cat
    End Select
    rIdioma.MoveNext
  Loop
  
  ' Cerramos la conexion
  
  conn.Close
  conn = Null
  
End

