Public Sub GetConfig()
  
  aConfigDias.Clear
  aConfigMeses.Clear
  
  conn = gConnection.ConectarDb()
  ' Traemos los datos de la configuración para aplicar en los calendarios
  rConfigDias = conn.Exec("select std.tipo,std.valor from staticdata std inner join config conf on std.id_config = conf.id where std.tipo = 'dia' and conf.seleccion = 1 order by std.id ")
  
  rConfigMeses = conn.Exec("select std.tipo,std.valor from staticdata std inner join config conf on std.id_config = conf.id where std.tipo = 'mes' and conf.seleccion = 1 order by std.id")
  
  ' cargamos los nombres de los meses en el array
  rConfigDias.MoveFirst
  Do While rConfigDias.Available
    aConfigDias.Add(rConfigDias!valor)
    rConfigDias.MoveNext
  Loop
  
  ' cargamos los nombres de los meses en el array
  rConfigMeses.MoveFirst
  Do While rConfigMeses.Available
    aConfigMeses.Add(rConfigMeses!valor)
    rConfigMeses.MoveNext
  Loop
  
  conn.Close
  conn = Null
  
End
