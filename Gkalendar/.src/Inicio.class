Public Sub Quatrimestre(val As Integer)
  ' Definimos las variables
  
  Dim strQuery1 As String
  Dim strQuery2 As String
  Dim strQuery3 As String
  Dim strQuery4 As String
  Dim i As Integer
  Dim mesTemp As Integer
  'Variable para items que menos de 1 caracter
  Dim zero As String = Null
  
  conn = gConnection.ConectarDb()
  ' Traspasamos los festivos a la tabla generada del año seleccionado
  rFestivos = conn.Exec("Select dia, mes, descripcion From festivos order by mes asc")
  'FIXME: Fiestas que aparecen debajo
  'TODO: Listado de festividades que aparecen en la parte inferior del calendario
  rFestivos.MoveFirst
  i = 1
  mesTemp = rFestivos!mes
  Do While rFestivos.Available
    If val = 1 Then 
      If mesTemp <> rFestivos!mes Then
        mesTemp = rFestivos!mes
        i = 1
      Endif
      ' Evaluamos si la iteración i tiene 2 digitos o 1 para añadir un 0
      If i < 10 Then
        zero = "0"
      Else
        zero = ""
      Endif
      If CInt(rFestivos!mes) = 1 Then 
        conn.Exec("update actualkal set FENERO = 1 where ENERO=" & rFestivos!dia) 
        aListaFId.Add(rFestivos!mes)       
        aListaFTag.Add("atxt" & zero & i)
        aListaFVal.Add("(" & rFestivos!dia & ") " & rFestivos!descripcion)
        i += 1        
      End If  
      
      If CInt(rFestivos!mes) = 2 Then 
        conn.Exec("update actualkal set FFEBRERO = 1 where FEBRERO=" & rFestivos!dia)
        aListaFId.Add(rFestivos!mes) 
        aListaFTag.Add("btxt" & zero & i)
        aListaFVal.Add("(" & rFestivos!dia & ") " & rFestivos!descripcion)
        i += 1   
