Public Sub pbCat_MouseDown()
  
  btnPrimero.visible = False
  btnSegundo.Visible = False
  btnTercero.Visible = False
  
  rdbCat.value = 1
  'NOTE: se actualiza la configuracinó de idioma
  conn = gConnection.ConectarDb()
  conn.Exec("update config set seleccion = 0 where numero = 1")
  conn.Exec("update config set seleccion = 1 where numero = 2")
  conn.Commit
  
  conn.Close
  conn = Null
  
  GetConfig()
  
End

