Public Sub btnAceptar_Click()
  '----------------------------------------------------------------------
  
  ' para devolver el número de día del a semana base 0
  ' *** Definicion de variables globales del módulo
  Dim anyo As Integer
  Dim DiasDelMes As Integer
  Dim NumDiaSemana As Integer
  Dim m As Integer
  Dim i As Integer 
  
  ' Inactivamos el botón de aceptar para evitar pulsaciones multiples
  btnAceptar.Enabled = False
  ' Se muestra el texto generando calendario...
  If rdbCat.Value = True Then
    txtTrabajo.Text = gDatosEstaticos.warning_esperando_cat
  Else
    txtTrabajo.Text = gDatosEstaticos.warning_esperando_sp 
  Endif
  
  txtTrabajo.visible = True
  
  Wait 0.01
  'Traemos el valor del campo Año introducido y lo parse/Cast a Integer
  anyo = CInt(txtAnyo.Text)
  
  GetConfig()
  
  Init()
  
  ' Abrimos la conexión a DB
  conn = gConnection.ConectarDb()
  
  'Insertamos los días de semana santa llamando al Procedimiento para que calcule los días correspondientes según año
  gSemanaSanta.FechaSemanaSanta(CInt(txtAnyo.text))
  
  ' Iteramos los 12 meses del año X
  For m = 1 To 12
    Select Case m
        'ENERO
      Case 1 
        'Dias que tiene el mes
        DiasDelMes = 31
        
        'Número de dia de la semana del día 1 de ENERO
        NumDiaSemana = WeekDay(CDate("01/01/" & Str(anyo))) + 1 
        ' Iteramos los días de cada mes desde el 1 hasta Dias del Mes
        For i = 1 To DiasDelMes
          conn.Begin
          conn.Exec("update actualkal set ENERO =" & i & " where id=" & NumDiaSemana) 
          NumDiaSemana += 1          
          conn.Commit        
        Next
        
        'FEBRERO
      Case 2
        DiasDelMes = gBisiesto.es_bisiesto(anyo)  
        'Número de dia de la semana del día 1 de FEBRERO
        NumDiaSemana = WeekDay(CDate("02/01/" & Str(anyo))) + 1
        ' Iteramos los días de cada mes desde el 1 hasta Dias del Mes
        For i = 1 To DiasDelMes
          conn.Exec("update actualkal set FEBRERO =" & i & " where id=" & NumDiaSemana) 
          NumDiaSemana += 1   
        Next 
        
        'MARZO  
      Case 3
        DiasDelMes = 31
        'Número de dia de la semana del día 1 de MARZO
        NumDiaSemana = WeekDay(CDate("03/01/" & Str(anyo))) + 1
        ' Iteramos los días de cada mes desde el 1 hasta Dias del Mes
        For i = 1 To DiasDelMes
          conn.Exec("update actualkal set MARZO =" & i & " where id=" & NumDiaSemana) 
          NumDiaSemana += 1   
        Next
        
        'ABRIL 
      Case 4
        DiasDelMes = 30
        'Número de dia de la semana del día 1 de ABRIL
        NumDiaSemana = WeekDay(CDate("04/01/" & Str(anyo))) + 1
        ' Iteramos los días de cada mes desde el 1 hasta Dias del Mes
        For i = 1 To DiasDelMes
          conn.Exec("update actualkal set ABRIL =" & i & " where id=" & NumDiaSemana) 
          NumDiaSemana += 1   
        Next
        'MAYO 
      Case 5
        DiasDelMes = 31 
        'Número de dia de la semana del día 1 de MAYO
        NumDiaSemana = WeekDay(CDate("05/01/" & Str(anyo))) + 1
        ' Iteramos los días de cada mes desde el 1 hasta Dias del Mes
        For i = 1 To DiasDelMes
          conn.Exec("update actualkal set MAYO =" & i & " where id=" & NumDiaSemana) 
          NumDiaSemana += 1   
        Next
        'JUNIO 
      Case 6
        DiasDelMes = 30
        'Número de dia de la semana del día 1 de JUNIO
        NumDiaSemana = WeekDay(CDate("06/01/" & Str(anyo))) + 1
        ' Iteramos los días de cada mes desde el 1 hasta Dias del Mes
        For i = 1 To DiasDelMes
          conn.Exec("update actualkal set JUNIO =" & i & " where id=" & NumDiaSemana) 
          NumDiaSemana += 1   
        Next
        'JULIO 
      Case 7
        DiasDelMes = 31
        'Número de dia de la semana del día 1 de JULIO
        NumDiaSemana = WeekDay(CDate("07/01/" & Str(anyo))) + 1
